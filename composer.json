{
    "name": "jonnitto/smartlook",
    "type": "neos-plugin",
    "description": "Integrate smartlook.com into your Neos CMS Site",
    "license": "GPL-3.0",
    "keywords": [
        "smartlook",
        "tracking",
        "recording"
    ],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "^5.3 || ^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\Smartlook\\": "Classes"
        }
    },
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitignore",
            ".github",
            ".gitattributes",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "extra": {
        "installer-name": "Jonnitto.Smartlook",
        "neos": {
            "package-key": "Jonnitto.Smartlook"
        }
    }
}
