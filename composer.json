{
    "name": "jonnitto/smartlook",
    "type": "neos-plugin",
    "description": "Integrate smartlook.com into your Neos CMS Site",
    "license": "MIT",
    "keywords": ["smartlook", "tracking", "recording"],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "~3.0 || ~4.0 || ~5.0 || ~7.0"
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\Smartlook\\": "Classes"
        }
    },
    "archive": {
        "exclude": [".editorconfig", ".gitignore", ".github", ".gitattributes", "CODE_OF_CONDUCT.md"]
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Jonnitto.Smartlook",
        "neos": {
            "package-key": "Jonnitto.Smartlook"
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
